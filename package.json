{
  "name": "pasienky",
  "version": "1.0.0",
  "description": "Scraper for Pasienky swimming pool schedule",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts --watch src --ext ts,js,json",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop pasienky-app",
    "pm2:restart": "pm2 restart pasienky-app",
    "pm2:delete": "pm2 delete pasienky-app",
    "pm2:logs": "pm2 logs pasienky-app",
    "pm2:monit": "pm2 monit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
