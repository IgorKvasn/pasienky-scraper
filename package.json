  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^22.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "xlsx": "^0.18.5",
    "pm2": "^5.4.0"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
